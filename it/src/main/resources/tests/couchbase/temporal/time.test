{
  "name": "filter on time_of_day (Couchbase)",

  "NB": "Couchbase's millis_to_utc doesn't show fractional part when milliseconds are zero",

  "backends": {
    "mongodb_2_6":       "skip",
    "mongodb_3_0":       "skip",
    "mongodb_read_only": "skip",
    "mongodb_3_2":       "skip",
    "postgresql":        "skip",
    "marklogic":         "skip"
  },

  "data": "../../days.data",

  "query": "select day, time_of_day(ts) as tod, time_of_day(day) as `not a date`, time_of_day(missing) as missing from `../../days`
    where time_of_day(ts) >= time(\"08:00\") and time_of_day(ts) < time(\"10:20:30.400\")",

  "predicate": "containsExactly",
  "ignoreFieldOrder": [ "couchbase" ],
  "expected": [
      { "day": "Tuesday",   "tod": "08:00:00", "not a date": {"$na": null}, "missing": {"$na": null} },
      { "day": "Wednesday", "tod": "09:00:00", "not a date": {"$na": null}, "missing": {"$na": null} },
      { "day": "Thursday",  "tod": "10:00:00", "not a date": {"$na": null}, "missing": {"$na": null} }]
}
