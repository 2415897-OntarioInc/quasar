{
    "name": "filter with pipeline using variables in query",

    "backends": {
        "mimir": "skip",
        "postgresql": "pending"
    },

    "data": "smallZips.data",

    "variables": {
        "cutoff": "1000"
    },

    "query": "select count(*) as cnt from smallZips where pop < :cutoff",

    "predicate": "exactly",

    "expected": [{ "cnt": 19 }]
}
